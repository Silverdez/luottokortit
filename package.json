{
  "name": "sage",
  "private": true,
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "bud dev",
    "build": "bud build",
    "translate": "yarn translate:pot && yarn translate:update",
    "translate:pot": "wp i18n make-pot . ./resources/lang/sage.pot --include=\"app,resources\"",
    "translate:update": "for filename in ./resources/lang/*.po; do msgmerge -U $filename ./resources/lang/sage.pot; done; rm -f ./resources/lang/*.po~",
    "translate:compile": "yarn translate:mo && yarn translate:js",
    "translate:js": "wp i18n make-json ./resources/lang --pretty-print",
    "translate:mo": "wp i18n make-mo ./resources/lang ./resources/lang"
  },
  "devDependencies": {
    "@roots/bud": "5.7.7",
    "@roots/bud-babel": "^5.7.7",
    "@roots/bud-postcss": "^5.7.7",
    "@roots/bud-react": "^5.7.7",
    "@roots/bud-sass": "^5.7.7",
    "@roots/bud-tailwindcss": "5.7.7",
    "@roots/sage": "5.7.7",
    "@wordpress/babel-preset-default": "^6.7.0",
    "@wordpress/browserslist-config": "^4.1.2",
    "@wordpress/dependency-extraction-webpack-plugin": "^3.4.1",
    "babel-eslint": "^10.1.0",
    "browser-sync": "^2.27.9",
    "browser-sync-webpack-plugin": "^2.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.26.0",
    "glslify-loader": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "purgecss-with-wordpress": "^4.1.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
    "stylelint": "^14.6.1",
    "stylelint-config-standard": "^25.0.0",
    "vue-template-compiler": "^2.6.14"
  },
  "dependencies": {
    "@barba/core": "^2.9.7",
    "@barba/prefetch": "^2.1.10",
    "@barba/router": "^2.1.10",
    "@glidejs/glide": "^3.5.2",
    "@popperjs/core": "^2.11.5",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "baffle": "^0.3.6",
    "charming": "^3.0.2",
    "dat.gui": "^0.7.9",
    "glsl-noise": "^0.0.0",
    "glslify": "^7.1.1",
    "gsap": "^3.10.2",
    "jquery": "^3.6.0",
    "lottie-web": "^5.9.3",
    "object-fit-images": "^3.2.4",
    "polished": "^4.2.2",
    "popper.js": "^1.16.1",
    "postprocessing": "^6.26.3",
    "scroll-restoration-polyfill": "^0.6.1",
    "susy": "^3.0.7",
    "three": "^0.139.2",
    "tiny-emitter": "^2.1.0"
  }
}
